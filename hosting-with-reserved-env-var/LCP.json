{
  "id": "ui",
  "loadBalancer": {
    "targetPort": 80
  },
  "readinessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "livenessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "memory": 512,
  "cpu": 2,
  "env": {
    "LCP_CONTAINER_CPU": "1"
  }
}
