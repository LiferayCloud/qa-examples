{
  "id": "ui",
  "loadBalancer": {
    "targetPort": 80
  },
  "readinessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "livenessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "memory": 512,
  "cpu": 1,
  "volumes": {
    "storage1": "/opt/storage1",
    "storage2": "/opt/storage2",
    "storage3": "/opt/storage3",
    "storage4": "/opt/storage4",
    "storage5": "/opt/storage5",
    "storage6": "/opt/storage6"
  }
}
