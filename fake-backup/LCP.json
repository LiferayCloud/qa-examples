{
  "id": "backup",
  "image": "liferaycloud/backup:5.3.1",
  "loadBalancer": {
    "targetPort": 80
  },
  "readinessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "livenessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "memory": 512,
  "cpu": 1
}